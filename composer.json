{
    "name": "itk-event-database/event-database-api",
    "license": "MIT",
    "type": "project",
    "description": "The Event Database API",
    "homepage": "https://github.com/itk-event-database/event-database-api",
    "authors": [
        {
            "name": "Mikkel Ricky",
            "email": "rimi@aarhus.dk"
        },
        {
            "name": "Ture Gj√∏rup",
            "email": "tug@aarhus.dk"
        }
    ],
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": "~7.0.0",
        "symfony/symfony": "3.4.*",
        "api-platform/core": "^2.2",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "dunglas/action-bundle": "^0.3",
        "nelmio/cors-bundle": "^1.4",
        "phpdocumentor/reflection-docblock": "^3.0",
        "doctrine/data-fixtures": "^1.2",
        "hautelook/alice-bundle": "^1.2",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "lexik/jwt-authentication-bundle": "^1.7",
        "stof/doctrine-extensions-bundle": "^1.2",
        "guzzlehttp/guzzle": "^6.2",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "symfony/property-access": "^3.1",
        "fpn/tag-bundle": "^0.9.0",
        "league/uri": "^4.1",
        "skyscanner/jsonpath": "^0.7.1",
        "psr/log": "1.0.0",
        "resquebundle/resque": "^1.0",
        "exercise/htmlpurifier-bundle": "^0.2.3",
        "sebastian/diff": "^1.4",
        "vich/uploader-bundle": "^1.5",
        "egeloen/ckeditor-bundle": "^4.0",
        "itk-dev/database-bundle": "^1.0",
        "easycorp/easyadmin-bundle": "^1.17"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "behat/behat": "^3.1",
        "behat/symfony2-extension": "^2.1",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.2",
        "behat/mink-browserkit-driver": "^1.3.1",
        "behatch/contexts": "^2.5",
        "doctrine/data-fixtures": "^1.1",
        "hautelook/alice-bundle": "^1.2",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "phpunit/phpunit": "^5.6",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "@install-git-hooks"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "check-coding-standards/phpcs": [
            "vendor/bin/phpcs --standard=phpcs.xml.dist"
        ],
        "check-coding-standards": [
            "@check-coding-standards/phpcs"
        ],
        "apply-coding-standards/phpcs": [
            "vendor/bin/phpcbf --standard=phpcs.xml.dist"
        ],
        "apply-coding-standards": [
            "@apply-coding-standards/phpcs"
        ],
        "install-git-hooks": [
            "ln -sf ../../githooks/pre-commit .git/hooks"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
